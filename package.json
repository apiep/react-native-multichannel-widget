    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "mitt": "^3.0.0",
    "moment": "^2.29.1",
    "qiscus-sdk-core": "^2.11.1",
    "react-i18next": "^11.8.13",
    "react-native-autolink": "^4.0.0",
    "react-native-hyperlink": "^0.0.19",
    "react-native-image-progress": "^1.1.1",
