    "react-native-web": "~0.13.12",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.0.0",
    "typescript": "~4.0.0",
    "unimodules-permissions-interface": "^6.1.0"
  },
  "private": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
