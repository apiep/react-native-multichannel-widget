{
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "lib": [
      "ES2017"
    ],
    "typeRoots": [
      "node_modules/@types",
      "lib/types"
    ],
    "strictNullChecks": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
